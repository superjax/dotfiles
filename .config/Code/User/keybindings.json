// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "f4",
        "command": "-editor.action.nextMatchFindAction",
        "when": "editorTextFocus"
    },
    {
        "key": "f4",
        "command": "-search.action.focusNextSearchResult",
        "when": "hasSearchResult"
    },
    {
        "key": "f4",
        "command": "-goToNextReference",
        "when": "referenceSearchVisible"
    },
    {
        "key": "f4",
        "command": "-goToNextReferenceFromEmbeddedEditor",
        "when": "inReferenceSearchEditor"
    },
    {
        "key": "f4",
        "command": "-references-view.next",
        "when": "reference-list.hasResult"
    },
    {
        "key": "f4",
        "command": "C_Cpp.SwitchHeaderSource",
        "when": "editorTextFocus && editorLangId == 'cpp'"
    },
    {
        "key": "alt+o",
        "command": "-C_Cpp.SwitchHeaderSource",
        "when": "editorTextFocus && editorLangId == 'cpp'"
    },
    {
        "key": "f4",
        "command": "C_Cpp.SwitchHeaderSource",
        "when": "editorTextFocus && editorLangId == 'c'"
    },
    {
        "key": "alt+o",
        "command": "-C_Cpp.SwitchHeaderSource",
        "when": "editorTextFocus && editorLangId == 'c'"
    },
    {
        "key": "ctrl+shift+b",
        "command": "-workbench.action.tasks.build"
    },
    {
        "key": "ctrl+shift+b",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+b",
        "command": "extension.rerunLastCommand"
    },
    {
        "key": "ctrl+f7",
        "command": "-extension.rerunLastCommand"
    },
    {
        "key": "ctrl+\\",
        "command": "-workbench.action.splitEditor"
    },
    {
        "key": "ctrl+shift+w",
        "command": "workbench.files.action.showActiveFileInExplorer"
    },
    {
        "key": "ctrl+shift+w",
        "command": "-workbench.action.closeWindow"
    },
    {
        "key": "ctrl+shift+d",
        "command": "-workbench.view.debug"
    },
    {
        "key": "ctrl+shift+d",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+down",
        "command": "-editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+down",
        "command": "-editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+up",
        "command": "-editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+w",
        "command": "remote-wsl.revealInExplorer",
        "when": "false"
    },
    {
        "key": "shift+alt+r",
        "command": "-remote-wsl.revealInExplorer",
        "when": "false"
    },
    {
        "key": "ctrl+shift+o",
        "command": "workbench.action.files.revert"
    },
    {
        "key": "ctrl+shift+b",
        "command": "aurora-vscode.jumpToBuild"
    },
    {
        "key": "ctrl+alt+o o",
        "command": "-vscode-open.open"
    },
    {
        "key": "ctrl+alt+o",
        "command": "vscode-open.openLines"
    },
    {
        "key": "ctrl+alt+o l",
        "command": "-vscode-open.openLines"
    },
    {
        "key": "ctrl+alt+o u",
        "command": "-vscode-open.openURI"
    },
    {
        "key": "ctrl+alt+o p",
        "command": "-vscode-open.openPR"
    },
    {
        "key": "ctrl+shift+o",
        "command": "-workbench.action.gotoSymbol"
    },
    {
        "key": "ctrl+k right",
        "command": "-workbench.action.moveActiveEditorGroupRight"
    },
    {
        "key": "ctrl+alt+l",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "ctrl+k ctrl+right",
        "command": "-workbench.action.focusRightGroup"
    },
    {
        "key": "ctrl+alt+h",
        "command": "workbench.action.focusLeftGroup"
    },
    {
        "key": "ctrl+alt+j",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+alt+u",
        "command": "workbench.action.splitEditorDown"
    },
    {
        "key": "ctrl+alt+i",
        "command": "workbench.action.splitEditorLeft"
    },
    {
        "key": "ctrl+alt+k",
        "command": "workbench.action.focusLastEditorGroup"
    },
    {
        "key": "ctrl+t",
        "command": "-workbench.action.showAllSymbols"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.gotoSymbol"
    }
    {
        "key": "ctrl+shift+t",
        "command": "editor.action.insertSnippet",
        "args": {"snippet": "fprintf(stderr, \"%s:%d $1\\n\", __FILE__, __LINE__$2);\n$3"}
    }
]
